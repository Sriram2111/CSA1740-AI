from itertools import permutations

def solve_cryptarithmetic():
    letters = 'SENDMORY'

    for perm in permutations('0123456789', len(letters)):
        s, e, n, d, m, o, r, y = perm
        if s == '0' or m == '0': 
            continue  
        send = int(s + e + n + d)
        more = int(m + o + r + e)
        money = int(m + o + n + e + y)

        if send + more == money:
            print(f"SEND: {send}")
            print(f" MORE: {more}")
            print(f"MONEY: {money}")
            return


solve_cryptarithmetic()

